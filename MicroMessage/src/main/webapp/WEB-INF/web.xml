<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
    <servlet-name>ListServlet</servlet-name>
    <servlet-class>com.acehouhao.servlet.ListServlet</servlet-class>
  </servlet>
    <servlet>
        <servlet-name>DeleteOneServlet</servlet-name>
        <servlet-class>com.acehouhao.servlet.DeleteOneServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteBatchServlet</servlet-name>
        <servlet-class>com.acehouhao.servlet.DeleteBatchServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>InitTalkServlet</servlet-name>
        <servlet-class>com.acehouhao.servlet.InitTalkServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AutoReplyServlet</servlet-name>
        <servlet-class>com.acehouhao.servlet.AutoReplyServlet</servlet-class>
    </servlet>
  
  <servlet-mapping>
    <servlet-name>ListServlet</servlet-name>
    <url-pattern>/List.action</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>DeleteOneServlet</servlet-name>
    <url-pattern>/DeleteOneServlet.action</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>DeleteBatchServlet</servlet-name>
    <url-pattern>/DeleteBatchServlet.action</url-pattern>
  </servlet-mapping>
    <servlet-mapping>
        <servlet-name>InitTalkServlet</servlet-name>
        <url-pattern>/InitTalkServlet.action</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AutoReplyServlet</servlet-name>
        <url-pattern>/AutoReplyServlet.action</url-pattern>
    </servlet-mapping>
</web-app>
